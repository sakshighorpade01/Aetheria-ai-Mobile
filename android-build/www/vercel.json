{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://aios-web-production.up.railway.app/api/:path*"
    },
    {
      "source": "/login/:path*",
      "destination": "https://aios-web-production.up.railway.app/login/:path*"
    },
    {
      "source": "/socket.io/:path*",
      "destination": "https://aios-web-production.up.railway.app/socket.io/:path*"
    }
  ]
}