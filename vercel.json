{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://aios-web-production.up.railway.app/api/:path*"
    },
    {
      "source": "/login/:path*",
      "destination": "https://aios-web-production.up.railway.app/login/:path*"
    },
    {
      "source": "/socket.io/:path*",
      "destination": "https://aios-web-production.up.railway.app/socket.io/:path*"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/assetlinks.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}